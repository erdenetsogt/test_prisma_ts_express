model PeopleAddress {
  id            Int      @id @default(autoincrement())
  peopleId      Int
  sumId         Int
  provinceId    Int
  homeaddress   String?
  mobile        String?
  fax           String?
  email         String?
  postAddress   String?
  contactPerson String?
  contactMobile String?
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt

  @@map("peopleAddresses")
}